{
    "name": "Kahoot multitab",
    "version": "0.0.0.1",
    "manifest_version": 3,
    "description": "Amazing.",
    "action": {
        "default_popup": "popup/popup.html"
    },
    "background": {
        "service_worker": "service-worker.js"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content-scripts/host.js", "content-scripts/client.js"]
        }
    ]

}